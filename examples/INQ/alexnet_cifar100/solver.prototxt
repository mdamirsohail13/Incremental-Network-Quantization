net: "./examples/INQ/alexnet_cifar100/alexnet_cifar100_train_val.prototxt"
test_iter: 100 # Batch 100 * 100 iter = 10K test data
test_interval: 500
base_lr: 0.003
lr_policy: "step"
gamma: 0.2
stepsize: 2000
display: 100
max_iter: 3000 # Batch 250 * 3000 iter
momentum: 0.9
weight_decay: 0.0005
snapshot: 1000
snapshot_prefix: "models/alexnet_cifar100/alexnet_cifar100_part2"
solver_mode: GPU
